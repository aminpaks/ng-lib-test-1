{
    "compilerOptions": {
        "target": "es2015",
        "module": "es2015",
        "moduleResolution": "node",
        "suppressImplicitAnyIndexErrors": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "stripInternal": true,
        "lib": [
            "dom",
            "es2015",
            "es2016",
            "es2017"
        ],
        "outDir": "./tsc-out",
        "baseUrl": "."
    }
}